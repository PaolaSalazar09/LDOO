<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>check</servlet-name>
        <servlet-class>Controlador.Check</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Singleton</servlet-name>
        <servlet-class>Factory.Singleton</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Usuario</servlet-name>
        <servlet-class>Modelo.Usuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuarioAdmin</servlet-name>
        <servlet-class>Modelo.UsuarioAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Invitado</servlet-name>
        <servlet-class>Modelo.Invitado</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Normal</servlet-name>
        <servlet-class>Modelo.Normal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registrar</servlet-name>
        <servlet-class>Controlador.Registrar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>check</servlet-name>
        <url-pattern>/check</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Singleton</servlet-name>
        <url-pattern>/Singleton</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Usuario</servlet-name>
        <url-pattern>/Usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioAdmin</servlet-name>
        <url-pattern>/UsuarioAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Invitado</servlet-name>
        <url-pattern>/Invitado</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Normal</servlet-name>
        <url-pattern>/Normal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registrar</servlet-name>
        <url-pattern>/Registrar</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
